{
    "name": "teknoo/east-code-runner",
    "description": "East bundle to run some code from request in secured and isolated environment for tests, demos or async operations",
    "license": "MIT",
    "type": "library",
    "keywords": ["east","remote","execution","async","worker"],
    "homepage": "http://teknoo.software/code-runner",
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\CodeRunner\\": "src/universal",
            "Teknoo\\East\\CodeRunnerBundle\\": "src/symfony"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\East\\CodeRunner\\": "tests/universal",
            "Teknoo\\Tests\\East\\CodeRunnerBundle\\": "tests/symfony"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "ext-bcmath": "*",
        "doctrine/orm": "~2.2",
        "gedmo/doctrine-extensions": "~2.4",
        "teknoo/immutable": "^0.0.1",
        "teknoo/east-foundation": "dev-master",
        "teknoo/states": "^3.0.0",
        "teknoo/states-bundle": "^3.0.0",
        "teknoo/states-life-cycle": "^2.0.0",
        "container-interop/service-provider":"^0.3.0",
        "knplabs/gaufrette": "~0.2",
        "adambrett/shell-wrapper": "dev-master",
        "guzzlehttp/psr7": "^1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "squizlabs/php_codesniffer": "~2.8",
        "pdepend/pdepend": "~2.5",
        "phpmd/phpmd": "~2.6",
        "sebastian/phpcpd": "~3.0",
        "phploc/phploc": "~3.0",
        "covex-nn/phpcb": "~1.0",
        "php-amqplib/php-amqplib": "^2.6.0",
        "php-amqplib/rabbitmq-bundle": "^1.11.0",
        "symfony/symfony": "^3.0.0",
        "stof/doctrine-extensions-bundle": "~1.0"
    },
    "minimum-stability": "stable"
}
