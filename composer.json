    "name": "teknoo/east-code-runner",
    "description": "East Symfony bundle to run PHP code from request in secure environment",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["east","remote execution"],
    "homepage": "http://teknoo.software/code-runner",
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\CodeRunner\\": "src/universal",
            "Teknoo\\East\\CodeRunnerBundle\\": "src/symfony"
        }
