        "teknoo/states-bundle": "^3.0.0-beta1",
        "teknoo/states-life-cycle": "^2.0.0-beta1",
        "container-interop/service-provider":"^0.3.0",
        "knplabs/gaufrette": "~0.2",
        "adambrett/shell-wrapper": "~0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "php-amqplib/php-amqplib": "^2.6.0",
